{
  "manifest_version": 2,
  "name": "ConsentAgent",
  "version": "1.0",
  "description": "",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_title": "ConsentAgent",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false,
    "type": "module"
  }
}