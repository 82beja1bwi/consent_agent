{
  "manifest_version": 2,
  "name": "ConsentAgent",
  "version": "1.0",
  "description": "",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "ConsentAgent",
    "default_popup": "popup/overview.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false,
    "type": "module"
  }
}